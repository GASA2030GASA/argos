    "supertest": "^4.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "modulePaths": [
