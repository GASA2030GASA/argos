    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^4.0.7",
    "helmet": "^3.21.3",
    "image-difference": "^2.0.7",
    "knex": "^0.20.11",
    "lodash": "^4.17.15",
    "merge-deep": "^3.0.2",
